<keymap>
  <global>
    <keyboard>
      <p>Play</p>
      <q>Queue</q>
      <f>FastForward</f>
      <r>Rewind</r>
      <h>Plex.ActivateWindow(Home)</h>
      <left>Left</left>
      <right>Right</right>
      <up>Up</up>
      <down>Down</down>
      <pageup>PageUp</pageup>
      <pagedown>PageDown</pagedown>
      <return>Select</return>
	  <enter>Select</enter>
      <backspace>ParentDir</backspace>
      <m>ActivateWindow(PlayerControls)</m>
      <s>Plex.ActivateWindowWithFocus(Home,300,1)</s>
      <escape>PreviousMenu</escape>
      <i>Info</i>
      <c>ContextMenu</c>
      <space>Pause</space>
      <x>Stop</x>
      <period>SkipNext</period>
      <comma>SkipPrevious</comma>
